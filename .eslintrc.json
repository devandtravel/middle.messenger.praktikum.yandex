{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": ["airbnb"],
    "parser": "@typescript-eslint/parser",
    "plugins": ["prettier", "@typescript-eslint"],
    "rules": {
        "max-len": [2, 100],
        "max-params": [2, 3],
        "@typescript-eslint/no-unused-vars": 2
    }
}
